{
  "dashboard": {
    "id": null,
    "title": "VisionFlow Enhanced Video Generation Dashboard",
    "tags": ["visionflow", "video-generation", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Video Generation Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(visionflow_video_generation_jobs_active)",
            "legendFormat": "Total Active Jobs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Success Rate by Quality",
        "type": "gauge",
        "targets": [
          {
            "expr": "visionflow_video_generation_success_rate",
            "legendFormat": "{{quality}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Queue Length Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "visionflow_video_generation_queue_length",
            "legendFormat": "Queue Length"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Average Wait Time by Quality",
        "type": "barchart",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, visionflow_video_generation_wait_time_seconds_bucket)",
            "legendFormat": "{{quality}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Active Jobs by Status and Quality",
        "type": "table",
        "targets": [
          {
            "expr": "visionflow_video_generation_jobs_active",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true
              },
              "renameByName": {
                "quality": "Quality",
                "status": "Status",
                "Value": "Count"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center"
            }
          }
        },
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "Video Generation Duration Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(visionflow_video_generation_duration_seconds_bucket[5m])) by (le, quality)",
            "format": "heatmap"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "scheme"}
          }
        },
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Real-time Job Progress",
        "type": "timeseries",
        "targets": [
          {
            "expr": "visionflow_video_generation_job_progress",
            "legendFormat": "{{job_id}} - {{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 10}
      },
      {
        "id": 8,
        "title": "Error Rate by Service",
        "type": "piechart",
        "targets": [
          {
            "expr": "rate(visionflow_errors_total[5m])",
            "legendFormat": "{{service}} - {{error_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 9,
        "title": "Request Duration by Service",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, visionflow_request_duration_seconds_bucket)",
            "legendFormat": "{{service}} - 95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 6, "w": 16, "x": 8, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1,
    "links": [],
    "annotations": {
      "list": []
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "templating": {
      "list": [
        {
          "name": "quality",
          "type": "query",
          "query": "label_values(visionflow_video_generation_jobs_active, quality)",
          "refresh": 2,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "status",
          "type": "query",
          "query": "label_values(visionflow_video_generation_jobs_active, status)",
          "refresh": 2,
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}
