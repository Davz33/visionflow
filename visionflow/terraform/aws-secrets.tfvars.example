# AWS Secrets Configuration Example for VisionFlow
# Copy this file to aws-secrets.tfvars and update with your actual values

# AWS Secrets Manager Configuration
aws_secrets = {
  aws-access-key-id = {
    secret_data = "YOUR_AWS_ACCESS_KEY_ID"
    labels = {
      service = "aws"
      type    = "credentials"
    }
  }
  aws-secret-access-key = {
    secret_data = "YOUR_AWS_SECRET_ACCESS_KEY"
    labels = {
      service = "aws"
      type    = "credentials"
    }
  }
  aws-key-pair = {
    secret_data = "your-key-pair-name"
    labels = {
      service = "aws"
      type    = "ssh-key"
    }
  }
}
